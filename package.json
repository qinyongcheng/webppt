{
	"name": "webppt",
	"version": "1.0.4",
	"files": [
		"dist"
	],
	"main": "./dist/webppt.umd.js",
	"module": "./dist/webppt.es.js",
	"unpkg": "./dist/webppt.min.js",
	"style": "./dist/index.css",
	"exports": {
		".": {
			"import": "./dist/webppt.es.js",
			"require": "./dist/webppt.umd.js"
		},
		"./dist/index.css": {
			"import": "./dist/index.css",
			"require": "./dist/index.css"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc --noEmit && vite build",
		"build-npm": "vue-tsc --noEmit && vite build --mode npm",
		"publish-release": "npm run build-npm && npm publish --access public",
		"publish-beta": "npm run publish-release --tag beta",
		"preview": "vite preview"
	},
	"dependencies": {
		"animate.css": "^4.1.1",
		"magic.css": "^1.4.8",
		"n-zepto": "^1.2.0",
		"screenfull": "^6.0.2"
	},
	"devDependencies": {
		"@rollup/plugin-inject": "^5.0.3",
		"@vitejs/plugin-vue": "^1.6.0",
		"@vue/compiler-sfc": "^3.2.6",
		"typescript": "^4.7.4",
		"vue": "^3.2.8",
		"vite": "^2.5.2",
		"vue-tsc": "^1.2.0"
	}
}
